[Unit]
Description=Prometheus exporter for wormcells-viz logs
Requires=cengen.service
Requires=bendavid2021.service
Requires=packer2019.service

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/grok_exporter
ExecStart=/home/ubuntu/grok_exporter/grok_exporter -config /home/ubuntu/grok_exporter/config.yml

[Install]
WantedBy=multi-user.target